@model WorkManagementWeb.Models.Team

@{
    ViewData["Title"] = "TGrpup";
    Layout = "~/Views/Template/TeamTemplate.cshtml";
}

<h1>TGrpup</h1>

<h4>Güncelle</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="TGrpup">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="T_ID" />
            <div class="form-group">
                <label asp-for="TeamName" class="control-label"></label>
                <input asp-for="TeamName" class="form-control" />
                <span asp-validation-for="TeamName" class="text-danger"></span>
            </div>
            
          
            <div class="form-group">
                <input type="submit" value="Kaydet" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="TeamList">Geri Dön</a>
</div>

 