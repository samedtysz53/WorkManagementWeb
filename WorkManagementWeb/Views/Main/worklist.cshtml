@{
    ViewData["Title"] = "WorkList";
    Layout = "~/Views/Template/Template.cshtml";
}

@model List<WorkManagementWeb.Models.JoblistModels>
<h4  class="text-white">
@ViewBag.User
</h4>
<ul class="list-unstyled">
    @if (Model != null)
    {
        @foreach (var item in Model)
        {
            <li class="mt-4 d-flex justify-content-center">
                @Html.ActionLink(item.JobListName, "Sonuc", new { id = item.JobListName }, new { @class = "btn btn-primary w-50 text-center" })
            </li>
        }

    }
    else
    {

    }
</ul>


